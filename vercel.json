{
  "rewrites": [
    {
      "source": "/seed",
      "destination": "/seed/index.html"
    },
    {
      "source": "/seed/(.*)",
      "destination": "/seed/index.html"
    },
    {
      "source": "/open",
      "destination": "/open/index.html"
    },
    {
      "source": "/open/(.*)",
      "destination": "/open/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}